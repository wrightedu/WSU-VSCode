{
	"name": "wsu-cse",
	"displayName": "WSU CSE",
	"description": "Universal VSCode plugin for classes in the Wright State CSE department",
	"version": "0.0.12",
	"publisher": "WrightStateUniversity-ComputerScience",
	"author": {
		"name": "Wright State University"
	},
	"homepage": "https://github.com/wrightedu/WSU-VSCode",
	"repository": {
		"type": "git",
		"url": "https://github.com/wrightedu/WSU-VSCode"
	},
	"engines": {
		"vscode": "^1.69.2"
	},
	"keywords": [
		"wsu",
		"wright state",
		"wright state university"
	],
	"categories": [
		"Other",
		"Extension Packs"
	],
	"activationEvents": [
		"onCommand:wsuvscode.makeProject",
		"onCommand:wsuvscode.profileProject"
	],
	"main": "./src/wsuvscode.js",
	"contributes": {
		"commands": [
			{
				"command": "wsuvscode.makeProject",
				"title": "WSU: Make Project"
			},
			{
				"command": "wsuvscode.profileProject",
				"title": "WSU: Profile Project"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"eslint": "^8.13.0",
		"glob": "^7.1.5",
		"mocha": "^9.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"extensionDependencies": [
		"aaron-bond.better-comments",
		"ban.spellright",
		"danielpinto8zz6.c-cpp-compile-run",
		"ecmel.vscode-html-css",
		"ecodes.vscode-phpmd",
		"esbenp.prettier-vscode",
		"formulahendry.auto-close-tag",
		"formulahendry.auto-rename-tag",
		"ionutvmi.path-autocomplete",
		"kokororin.vscode-phpfmt",
		"ms-python.python",
		"ms-vscode.cpptools-extension-pack",
		"ms-vscode.makefile-tools",
		"ms-vscode-remote.remote-wsl",
		"oderwat.indent-rainbow",
		"PKief.material-icon-theme",
		"ritwickdey.LiveServer",
		"techer.open-in-browser",
		"visualstudioexptteam.vscodeintellicode",
		"vsciot-vscode.vscode-arduino",
		"vscjava.vscode-java-pack",
		"xdebug.php-debug",
		"zobo.php-intellisense"
	]
}
